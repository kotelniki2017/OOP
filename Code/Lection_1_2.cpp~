#include<iostream>
#include<stdlib.h>
using namespace std;

main()
{
  int **a, **b;
  int n,m;
  cout<<"Введите размерность массивов"<<endl;
  cout<<"n= ";
  cin>>n;
  cout<<"m= ";
  cin>>m;
  a = new int *[n];b = new int *[m];
  for (int i=0;i<n;i++) a[i] = new int [m];
  for (int i=0;i<m;i++) b[i] = new int [n];

  
  
